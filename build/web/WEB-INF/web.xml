<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file>
        <welcome-file-list>servletTest</welcome-file-list>
    </welcome-file>
    <servlet>
        <servlet-name>servletTest</servlet-name>
        <servlet-class>Controller.servletTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletPageLoginServeur</servlet-name>
        <servlet-class>Controller.servletPageLoginServeur</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>serveltLoginServeur</servlet-name>
        <servlet-class>Controller.serveltLoginServeur</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletPageLoginCuisinier</servlet-name>
        <servlet-class>Controller.servletPageLoginCuisinier</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>serveltLoginCuisiner</servlet-name>
        <servlet-class>Controller.serveltLoginCuisiner</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletTest</servlet-name>
        <url-pattern>/servletTest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletPageLoginServeur</servlet-name>
        <url-pattern>/servletPageLoginServeur</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>serveltLoginServeur</servlet-name>
        <url-pattern>/serveltLoginServeur</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletPageLoginCuisinier</servlet-name>
        <url-pattern>/servletPageLoginCuisinier</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>serveltLoginCuisiner</servlet-name>
        <url-pattern>/serveltLoginCuisiner</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
